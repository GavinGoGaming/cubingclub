)\n\n\n\n\n\n// Alg box\ndocument.querySelector('.close-alg-show').addEventListener('click', (event)=>{\n    document.querySelector('.alg-show').classList.add('hidden');\n    document.querySelector('.close-alg-show').classList.add('hidden');\n})\nconst githack = \"https://rawcdn.githack.com/gavingogaming/cubingclub/main/\";\nconst ghuc = \"https://raw.githubusercontent.com/gavingogaming/cubingclub/main/\";\nvar algData = null;\nfetch(ghuc + \"algs/all.json\")\n    .then(d => d.json())\n    .then(d => {\n        algData = d;\n\n        d.forEach(alg => {\n            var e = document.createElement('div');\n            e.className = \"alg-item\";\n            e.dataset.page = alg.page;\n            e.textContent = alg.title;\n            e.addEventListener('click', (evnt)=>{\n                fetch(ghuc + alg.page).then(d => d.text())\n                    .then(pg => {\n                        document.querySelector('.alg-show').classList.remove('hidden');\n                        document.querySelector('.close-alg-show').classList.remove('hidden');\n                        document.querySelector('.alg-show').innerHTML = pg;\n                    })\n            })\n            document.querySelector('.inner-algs').appendChild(e);\n        })\n    })\n    .catch((err) => {\n        document.querySelector('.inner-algs').insertAdjacentHTML('beforeend', `<span style=\"margin:auto;\">${err.message}</span>`)\n    })",
